        </moonSizeScale>

        <sunIsPrimary>
            <key value="0" time="0" />
            <key value="0" time="6.5" />
            <key value="1" time="6.5" />
            <key value="1" time="19.5" />
            <key value="0" time="19.5" />
            <key value="0" time="24" />
