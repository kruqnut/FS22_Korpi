                <color shaderParameter="colorMat1" color="0.829 0.829 0.829"/>
            </variation>
        </variations>

        <frame node="2" widthOffset="0.005" heightOffsetTop="0.005" heightOffsetBot="0.015"/>
    </licensePlate>

    <licensePlate filename="maps/licensePlates/licensePlatesFI.i3d" node="1" type="SQUARISH" width="0.130" height="0.130">
        <font size="0.05" scaleX="1" scaleY="1"/>

        <variations>
            <variation> <!-- TYÖKONE 123 ABC-->
                <value node="0|0" numerical="true" alphabetical="false" />
                <value node="0|1" numerical="true" alphabetical="false" />
                <value node="0|2" numerical="true" alphabetical="false" />

                <value node="0|3" character="_" locked="true" />
				
