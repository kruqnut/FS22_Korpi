        <customShaderDefaults>
            <customShaderMap        name="shapeArray" filename="$data/effects/cutter/arrays/cornheader/fruitArray12x28.dds"/>
            <customShaderVariation  name="alphaMap"/>
        </customShaderDefaults>
    </motionPathEffect>
